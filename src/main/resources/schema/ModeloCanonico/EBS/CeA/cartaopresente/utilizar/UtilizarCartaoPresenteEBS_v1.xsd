				<xsd:element name="Observacao" type="string" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="MetaInformacao" type="ns2:MetaInformacaoType" minOccurs="0" maxOccurs="1"/>
			</xsd:sequence>
		</xsd:complexType>

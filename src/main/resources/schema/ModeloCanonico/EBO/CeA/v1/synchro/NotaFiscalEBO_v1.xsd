					<xsd:documentation><![CDATA[
					NUM_RE_SISCOMEX - Número do registro de exportação (siscomex)
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="numero-nota-fiscal" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					NUMERO - Número do documento fiscal (num NF)
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>


			<xsd:element name="codigo-rementente-pfj" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					REMETENTE_PFJ_CODIGO - Código da pessoa.
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="serie-subserie" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					SERIE_SUBSERIE - Série / Subsérie do documento fiscal
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="codigo-sistema" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					SIS_CODIGO - Código do sistema
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="tipo" type="xsd:string" minOccurs="1"
				maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					TIPO - Tipo de Documento, Nulo ou N = Documento é normal.
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>


			<xsd:element name="observacao" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
						CA2_OBSERVACAO - Preencher se houver observações
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="transportador" type="tns:TransportadorType"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[

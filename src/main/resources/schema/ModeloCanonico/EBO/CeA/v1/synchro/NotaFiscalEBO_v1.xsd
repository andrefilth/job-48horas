<?xml version="1.0" encoding="UTF-8"?>

<xsd:schema xmlns="http://www.w3.org/2001/XMLSchema"
	targetNamespace="http://www.cea.com.br/EBO/CEA/v1" xmlns:tns="http://www.cea.com.br/EBO/CEA/v1"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified"
	attributeFormDefault="qualified" version="1.0">
	<xsd:complexType name="ArrayOfIDFType">
		<xsd:sequence>
			<xsd:element name="idf" type="tns:IDFType" minOccurs="1"
				maxOccurs="unbounded">
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="ArrayOfPedidosType">
		<xsd:sequence>
			<xsd:element name="pedido" type="tns:PedidoType"
				minOccurs="1" maxOccurs="1" />
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="ArrayOfEstoqueType">
		<xsd:sequence>
			<xsd:element name="estoque" type="tns:EstoqueType"
				minOccurs="0" maxOccurs="1" />
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="EstoqueType">
		<xsd:sequence>
			<xsd:element name="identificador-movimento" type="xsd:int"
				minOccurs="0" maxOccurs="1" />
			<xsd:element name="identificador-sku" type="xsd:long"
				minOccurs="0" maxOccurs="1" />
			<xsd:element name="numero-amostra-inspecao" type="xsd:int"
				minOccurs="0" maxOccurs="1" />
			<xsd:element name="numero-pedido" type="xsd:long"
				minOccurs="0" maxOccurs="1" />
			<xsd:element name="numero-processo-qualidade" type="xsd:int"
				minOccurs="0" maxOccurs="1" />
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="PedidoType">
		<xsd:sequence>
			<xsd:element name="numero-pedido" type="xsd:string" />
			<xsd:element name="formula-negocio" type="xsd:string" />
			<xsd:element name="classificacao" type="xsd:string" />
			<xsd:element name="classificacao-codigo" type="xsd:string" />
			<xsd:element name="valor-total" type="xsd:string" />
			<xsd:element name="valor-unitario" type="xsd:string" />
			<xsd:element name="quantidade-pecas" type="xsd:string" />
			<xsd:element name="numero-ncm" type="xsd:string" />
			<xsd:element name="valor-ipi" type="xsd:string" />
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="NotaAssociadaType">
		<xsd:sequence>
			<xsd:element name="dof-sequence" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					Exemplo:1 
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="data-emissao" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					Data de emissao da nota associada
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="serie-subserie" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					SERIE_SUBSERIE - Série / Subsérie do documento fiscal
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="numero" type="xsd:string" minOccurs="1"
				maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					Exemplo: 022489
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="codigo-emitente-pfj" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					Exemplo: 022489
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="codigo-edof" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					Exemplo: NF
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="valor-total-dof" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					Exemplo: 00000001448.88
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>

		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="VolumeCargaType">
		<xsd:sequence>
			<xsd:element name="especie" type="xsd:string" minOccurs="1"
				maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					Tipo do volumes, exemplo: "PECAS" 
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="marca" type="xsd:string" minOccurs="1"
				maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					Marca 
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="numeracao" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
				Ex: 000000000000000			 
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="quantidade" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
				Ex: 000000000			 
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="peso-bruto" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					PESO_BRUTO_KG - Peso bruto total das mercadorias da nf em kg
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="peso-liquido" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					PESO_LIQUIDO_KG - Peso liquido total das mercadorias da nf em kg
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>

		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="TransportadorType">
		<xsd:sequence>
			<xsd:element name="transportador-pfj-codigo" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					TRANSPORTADOR_PFJ_CODIGO - Código da pessoa.
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="indicador-responsabilidade-frete"
				type="xsd:string" minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					IND_RESP_FRETE - Indicador de responsabilidade pelo frete (1=remetente,2=destinatario)
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="marca-veiculo" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					MARCA_VEICULO - Marca do veículo
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="modelo-veiculo" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					MODELO_VEICULO - Modelo do veículo
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="placa-veiculo" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					NUM_PLACA_VEICULO - Número de registro (placas) do veículo transportador
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="codigo-uf-placa-veiculo" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					PLACA_VEICULO_UF_CODIGO - Código da unidade federativa
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="numero-embarque" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					numero de embarque
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="numero-veiculo" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					numero do veiculo
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="codigo-transportador-loc" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					ex: 001
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="codigo-via-transporte" type="xsd:int"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
						VT_CODIGO - Código da via de transporte.    (1,2,3,4,6,7,8,10)
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="volume" type="tns:VolumeCargaType"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
						Volume
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="AliquotaType">
		<xsd:sequence>
			<xsd:element name="aliquota-icms-diferencial" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					ALIQ_DIFA - Alíquota do ICMS quando há diferencial de alíquota
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="aliquota-icms" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					ALIQ_ICMS - Aicms = Alíquota de ICMS
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="aliquota-imposto-importacao" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					ALIQ_II - Alíquota do imposto de importação
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="aliquota-ipi" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					ALIQ_IPI - Aipi = Alíquota de IPI
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="aliquota-stf" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					ALIQ_STF - Alíquota ICMS de substituição tributária para frente
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="aliquota-stt" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					ALIQ_STT - Alíquota ICMS de substituição tributária para trás
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="IDFType">
		<xsd:sequence>
			<xsd:element name="aliquota" type="tns:AliquotaType"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					Dados das aliquotas
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="codigo-aplicacao-mercadoria" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					AM_CODIGO - Código de aplicação da mercadoria
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="codigo-fiscal-operacao-prestacao"
				type="xsd:string" minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					CFOP_CODIGO - Código fiscal de operação e prestação
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="controle-instrucao" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					CTRL_INSTRUCAO - Controle de instrução de importação do registro 
					(I=inclusao, A=alteração, E=exclusão)
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="numero-importado-dof" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					DOF_IMPORT_NUMERO - Número do documento importado atribuído pelo estabelecimento 
					informante
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="data-di" type="xsd:string" minOccurs="1"
				maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					DT_DI - 
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="codigo-entradasaida" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					ENTSAI_UNI_CODIGO - Código da Unidade de Medida ex.: peça=pc, par=par
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="codigo-estoque" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					ESTOQUE_UNI_CODIGO - Código da Unidade de Medida
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="codigo-finalidade-comercial" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					FIN_CODIGO - Código da finalidade comercial
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="numero-idf" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					IDF_NUM - Número sequencial do item de documento fiscal
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="texto-complementar-idf" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					IDF_TEXTO_COMPLEMENTAR - Texto complementar para o item. Sem editar
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="indicador-contabilizacao" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					IND_CONTABILIZACAO - Indica se o IDF debe ser contabilizado pelo SQL* Contabil
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="codigo-informante-estabelecimento"
				type="xsd:string" minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					INFORMANTE_EST_CODIGO - Código da pessoa estabelecimento   --- Concatena com ‘FX’
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="codigo-mercadoria" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					MERC_CODIGO - Buscar da tabela do Ris. Deve ser montada como: 
					fórmula de negócio + classificação + código
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="codigo-nbm" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					NBM_CODIGO - Código da NBM
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="codigo-natureza-operacao" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					NOP_CODIGO - Código da natureza da operação
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="numero-di" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					NUM_DI - 
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="codigo-om" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					OM_CODIGO - Código de origem da mercadoria
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="peso-bruto" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					PESO_BRUTO_KG - Peso bruto das mercadorias do item do DOF em kg
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="peso-liquido" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					PESO_LIQUIDO_KG - Peso liquido das mercadorias do item do DOF em kg
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="preco-total" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					PRECO_TOTAL
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="preco-unitario" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					PRECO_UNITARIO
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="quantidade" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					QTD - Quantidade do item da nota fiscal
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="codigo-situacao-icms" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					STC_CODIGO - Código de situação tributária da mercadoria quanto ao ICMS
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="codigo-situacao-ipi" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					STP_CODIGO - Código de situação tributária da mercadoria quanto ao IPI
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="subclasse-idf" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					SUBCLASSE_IDF - Subclasse do item de documento fiscal
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="tipo-complemento" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					TIPO_COMPLEMENTO - Se o DOF for de complemento, indica qual tipo de complemento  : 
					I=Imposto = ,P= Preço e PI = Preço e Imposto
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="codigo-unidade-fiscal" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					UNI_FISCAL_CODIGO - Código da Unidade de Medida. 
					Preencher:	Peça=PC e par=PAR 
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>

			<xsd:element name="valor-ajustes-preco-total" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					VL_AJUSTE_PRECO_TOTAL
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="valor-base-icms" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					VL_BASE_ICMS - Valor da base de ICMS
				]]></xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="valor-base-ipi" type="xsd:string"
				minOccurs="1" maxOccurs="1" nillable="true">
				<xsd:annotation>
					<xsd:documentation><![CDATA[
					VL_BASE_IPI - Bipi = Valor da base de IPI
				]]></xsd:documentation>
				</xsd:annotation>
